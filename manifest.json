{
    "manifest_version": 3,
    "name": "URL History Tracker",
    "description": "tracks user journey",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts" : [
      {
        "matches" : [ "http://*/*", "https://*/*" ],
          "js": ["index.js"]
      }
    ],
    "oauth2": {
      "client_id": "",
      "scopes": ["https://www.googleapis.com/auth/contacts.readonly"]
    },
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "permissions": [
        "webNavigation",
          "tabs",
          "history",
          "identity"
      ]
  }